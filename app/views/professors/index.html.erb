<div class = "container ">
	<% @professors.each do |professor| %>
	<div class = "row posting-inner">
		<div class = "col-md-offset-3 col-md-6 posting">
			<br>
			<div class = "row ">
				<div class = "col-md-12">
					<h1><%= professor.email %></h1>
				</div>
			</div>
			<div class = "row">
				<div class = "col-md-offset-10 col-md-2">
					<%= link_to 'Show', professor, :class => "btn btn-default", :type => "button", :method => :get  %>
				</div>
				<br><br><br>
			</div>
		</div>
	</div>
	<br><br>
	<% end %>
</div>